{
  "name": "voting-app",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "clean": "rimraf dist",
    "copy-deps": "copy-newer --cwd node_modules ** ../dist/node_modules",
    "start": "node ../dist/server/server.js",
    "serve": "concurrently \"npm run build.w\" \"nodemon ../dist/server/server.js\"",
    "lint": "tslint \"src/**/*.ts\"",
    "prebuild": "rimraf dist",
    "build": "npm run copy-deps && tsc --p ./src",
    "build.w": "npm run copy-deps && tsc --p ./src -w"
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.2",
    "express": "^4.14.0",
    "inversify": "^2.0.1",
    "inversify-express-utils": "^2.0.2",
    "mongodb": "^2.2.12",
    "morgan": "^1.7.0",
    "reflect-metadata": "^0.1.8"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.34",
    "@types/mongodb": "^2.1.36",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.0.42",
    "concurrently": "^3.1.0",
    "copy-newer": "^2.1.2",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4",
    "tslint": "3.13.0",
    "typescript": "2.1.1"
  }
}
